{
  "name": "@tauri-plugin/rpc",
  "version": "0.1.0",
  "description": "Type-safe RPC plugin for Tauri with React hooks",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    }
  },
  "files": ["dist", "bindings"],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "@tauri-apps/api": "^2.0.0",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "typescript": "^5.0.0"
  }
}
